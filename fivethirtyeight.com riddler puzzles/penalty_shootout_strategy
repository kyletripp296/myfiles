<?php

/*
____________________________
| zone 0 | zone 1 | zone 2 |
|   70%  |   90%  |   80%  |
|___________________________
| zone 3 | zone 4 | zone 5 |
|   80%  |  100%  |   90%  |
|__________________________|
*/


$sim = new Simulator();
$sim->play(3,1000);

class Simulator {
    private $make_percentages;
    private $zones;
    private $bots;
    
    public function __construct(){
        $this->make_percentages = array(70, 80, 90, 80, 100, 90);
        $this->zones = array();
        $this->bots = array();
        for($i=0;$i<=5;$i++){
            $this->zones[] = new Zone($i,$this->make_percentages[$i]);
        }
    }
    
    public function play($games = 1, $bots = 30){
        for($i=0;$i<$bots;$i++){
            $this->bots[] = new Bot(count($this->bots),mt_rand(0,5),mt_rand(0,5));
        }
        for($i=0;$i<$games;$i++){
            for($j=0;$j<count($this->bots);$j++){
                for($k=0;$k<count($this->bots);$k++){
                    if($j==$k){
                        continue;
                    }
                    $this->take_shot($this->bots[$j],$this->bots[$k]);
                }
            }
        }
        $this->print_bots_table();
    }
    
    protected function take_shot($bot1,$bot2){
        $s = $bot1->get_s();
        $g = $bot2->get_g();
        $save = ($s==$g) ? true : false;
        
        $this->zones[$s]->count_shooter();
        $this->zones[$g]->count_goalie();
        if($save){
            $this->zones[$g]->count_save();
            $bot1->count_loss();
            $bot2->count_win();
        }
        
        if(!$save){
            $p = $this->make_percentages[$s];
            $m = (mt_rand(0,99)<$p) ? true : false;
            
            if($m){
                $this->zones[$s]->count_goal();
                $bot1->count_win();
                $bot2->count_loss();
            } else {
                $this->zones[$s]->count_miss();
                $bot1->count_loss();
                $bot2->count_win();
            }
        }
    }
    
    protected function print_bots_table(){
        $bots = array();
        foreach($this->bots as $b){
            $bots[] = array(
                'id'=>$b->get_id(),
                's'=>$b->get_s(),
                'g'=>$b->get_g(),
                'w'=>$b->get_w(),
                'l'=>$b->get_l(),
            );
        }
        usort($bots,function($a,$b){
            return $b['w'] - $a['w'];
        });
        echo '<table><thead><tr><th>Bot #</th><th>Shoot at</th><th>Goalie At</th><th>Wins</th><th>Losses</th></tr></thead><tbody>';
        foreach($bots as $b){
            echo '<tr><td>'.$b['id'].'</td><td>'.$b['s'].'</td><td>'.$b['g'].'</td><td>'.$b['w'].'</td><td>'.$b['l'].'</td></tr>';
        }
        echo '</tbody></table>';
    }
}

class Zone {
    private $zone_id;
    private $make_pct;
    private $shooter_count;
    private $goalie_count;
    private $save_count;
    private $goal_count;
    private $miss_count;
    
    
    public function __construct($id,$pct){
        $this->zone_id = $id;
        $this->make_pct = $pct;
        $this->shooter_count = 0;
        $this->goalie_count = 0;
        $this->save_count = 0;
        $this->goal_count = 0;
        $this->miss_count = 0;
    }
    
    public function count_shooter(){
        $this->shooter_count++;
    }
    
    public function count_goalie(){
        $this->goalie_count++;
    }
    
    public function count_save(){
        $this->save_count++;
    }
    
    public function count_goal(){
        $this->goal_count++;
    }
    
    public function count_miss(){
        $this->miss_count++;
    }
}

class Bot {
    private $id;
    private $s;
    private $g;
    private $w;
    private $l;
    
    public function __construct($id, $s, $g){
        $this->id = $id;
        $this->s = $s;
        $this->g = $g;
    }
    
    public function count_win(){
        $this->w++;
    }
    
    public function count_loss(){
        $this->l++;
    }
    
    public function get_id(){
        return $this->id;
    }
    
    public function get_s(){
        return $this->s;
    }
    
    public function get_g(){
        return $this->g;
    }
    
    public function get_w(){
        return $this->w;
    }
    
    public function get_l(){
        return $this->l;
    }
}


